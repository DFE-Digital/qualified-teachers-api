{
  "ClientRateLimiting": {
    "EnableEndpointRateLimiting": false,
    "EndpointWhitelist": [
      "*:/health",
      "*:/status",
      "*:/swagger/*"
    ],
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Period": "60s",
        "Limit": 300
      }
    ]
  },
  "Serilog": {
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"
        }
      }
    ]
  }
}
